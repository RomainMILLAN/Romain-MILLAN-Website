{
    "type": "project",
    "name": "romainmillan/website",
    "description": "Website (portfolio) of Romain Millan",
    "version": "2.4.1",
    "keywords": ["symfony", "website", "portfolio"],
    "homepage": "https://romainmillan.fr/",
    "authors": [
        {
            "name": "Romain MILLAN",
            "email": "contact@romainmillan.fr",
            "homepage": "https://romainmillan.fr",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "contact@romainmillan.fr"
    },
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "arkounay/ux-collection": "^4.0",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.14",
        "doctrine/doctrine-migrations-bundle": "^3.4",
        "doctrine/orm": "^3.3",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.25",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "7.2.*",
        "symfony/console": "7.2.*",
        "symfony/dotenv": "7.2.*",
        "symfony/expression-language": "7.2.*",
        "symfony/flex": "^2",
        "symfony/form": "7.2.*",
        "symfony/framework-bundle": "7.2.*",
        "symfony/http-client": "7.2.*",
        "symfony/intl": "7.2.*",
        "symfony/joli-notif-notifier": "7.2.*",
        "symfony/mailer": "7.2.*",
        "symfony/mime": "7.2.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "7.2.*",
        "symfony/process": "7.2.*",
        "symfony/property-access": "7.2.*",
        "symfony/property-info": "7.2.*",
        "symfony/runtime": "7.2.*",
        "symfony/security-bundle": "7.2.*",
        "symfony/serializer": "7.2.*",
        "symfony/stimulus-bundle": "^2.20",
        "symfony/string": "7.2.*",
        "symfony/translation": "7.2.*",
        "symfony/twig-bundle": "7.2.*",
        "symfony/ux-autocomplete": "*",
        "symfony/ux-icons": "^2.23",
        "symfony/ux-live-component": "^2.21",
        "symfony/ux-turbo": "^2.21",
        "symfony/ux-twig-component": "^2.21",
        "symfony/validator": "7.2.*",
        "symfony/web-link": "7.2.*",
        "symfony/webpack-encore-bundle": "^2.2",
        "symfony/yaml": "7.2.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/AppContext/",
            "Front\\": "src/FrontContext/",
            "Panel\\": "src/PanelContext/",
            "Security\\": "src/SecurityContext/",
            "Signature\\": "src/SignatureContext/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/AppContext/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.2.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^4.1",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "7.2.*",
        "symfony/css-selector": "7.2.*",
        "symfony/debug-bundle": "7.2.*",
        "symfony/maker-bundle": "^1.62",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/stopwatch": "7.2.*",
        "symfony/web-profiler-bundle": "7.2.*",
        "symplify/coding-standard": "^12.2",
        "symplify/easy-coding-standard": "^12.3",
        "symplify/phpstan-rules": "^13.0"
    }
}
